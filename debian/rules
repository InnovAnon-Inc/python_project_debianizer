#!/usr/bin/make -f

# Force the use of the system's python3 build system
export PYBUILD_NAME={{ project_name }}

%:
	dh $@ --with python3 --buildsystem=pybuild

# This ensures we don't strip symbols too early if we want to debug 
# or if the Foundry needs them for AFDO mapping
override_dh_strip:
	dh_strip --no-automatic-dbgsym
